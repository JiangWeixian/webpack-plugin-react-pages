{
  "name": "webpack-local-module",
  "version": "0.0.0",
  "packageManager": "pnpm@7.23.0",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "homepage": "https://github.com/jiangwei.xian/webpack-local-module#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiangwei.xian/webpack-local-module.git"
  },
  "bugs": {
    "url": "https://github.com/jiangwei.xian/webpack-local-module/issues",
    "email": "jiangweixian1994@gmail.com"
  },
  "author": {
    "name": "JW",
    "email": "jiangweixian1994@gmail.com",
    "url": "https://twitter.com/jiangweixian"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build:lib": "rollup -c --silent",
    "build:dts": "tspc",
    "build": "rimraf dist && run-p build:*",
    "dev": "rollup -c -w --silent",
    "test": "vitest",
    "pretest": "pnpm run build"
  },
  "lint-staged": {
    "**/**/*.{js,ts,tsx,vue,json}": ["eslint --fix"]
  },
  "devDependencies": {
    "@aiou/eslint-config": "0.7.8",
    "@rollup/plugin-alias": "4.0.3",
    "@rollup/plugin-commonjs": "24.0.1",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rspack/core": "^0.5.4",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "20.11.17",
    "cz-emoji": "1.3.1",
    "esbuild": "^0.17.12",
    "fs-extra": "^11.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "3.0.2",
    "rollup": "3.19.1",
    "rollup-plugin-condition-exports": "1.3.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-filesize": "9.1.2",
    "rollup-plugin-node-externals": "5.1.2",
    "ts-patch": "3.1.1",
    "typescript": "5.3.3",
    "typescript-transform-paths": "3.4.6",
    "vitest": "0.10.0"
  }
}
