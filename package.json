{
  "name": "webpack-plugin-react-pages",
  "version": "0.1.2",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "homepage": "https://github.com/jiangWeixian/webpack-plugin-react-pages#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiangWeixian/webpack-plugin-react-pages.git"
  },
  "bugs": {
    "url": "https://github.com/jiangWeixian/webpack-plugin-react-pages/issues",
    "email": "jiangweixian1994@gmail.com"
  },
  "author": {
    "name": "JW",
    "email": "jiangweixian1994@gmail.com",
    "url": "https://twitter.com/jiangweixian"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./shim": {
      "types": "./shim.d.ts"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "shim.d.ts"
  ],
  "typesVersions": {
    "*": {
      "shim": [
        "./shim.d.ts"
      ]
    }
  },
  "scripts": {
    "build:lib": "rollup -c --silent",
    "build:dts": "tsc",
    "build": "rimraf dist && pnpm run build:dts && pnpm run build:lib",
    "dev": "rollup -c -w --silent",
    "play": "pnpm --filter=playground run dev",
    "test": "vitest",
    "pretest": "pnpm run build",
    "ci:version": "pnpm changeset version",
    "ci:publish": "pnpm run build && pnpm changeset publish",
    "ci:snapshot": "pnpm changeset version --snapshot beta",
    "ci:prerelease": "pnpm run build && pnpm changeset publish --tag beta",
    "prepare": "husky install",
    "lint:fix": "eslint . --fix"
  },
  "lint-staged": {
    "**/**/*.{js,ts,tsx,vue,json}": [
      "eslint --fix"
    ]
  },
  "peerDependencies": {
    "react-router": ">=6",
    "react-router-dom": ">=6",
    "webpack": ">=5"
  },
  "peerDependenciesMeta": {
    "react-router": {
      "optional": true
    },
    "react-router-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "debug": "^4.3.4",
    "pathe": "^0.2.0",
    "webpack-virtual-modules": "^0.4.2"
  },
  "devDependencies": {
    "@aiou/eslint-config": "^0.5.1",
    "@changesets/cli": "^2.21.1",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/debug": "^4.1.7",
    "@types/jest": "27.0.2",
    "@types/lodash": "4.14.181",
    "@types/node": "^17.0.22",
    "commitizen": "^4.2.4",
    "cz-emoji": "^1.3.1",
    "esbuild": "^0.14.48",
    "eslint": "^8.11.0",
    "esrua": "^0.2.1",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "lodash": "^4.17.21",
    "prettier": "2.4.1",
    "react-router": "6.3.0",
    "rimraf": "3.0.2",
    "rollup": "2.72.1",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-node-externals": "^4.0.0",
    "rollup-plugin-size": "^0.2.2",
    "tslib": "2.3.1",
    "typescript": "4.4.3",
    "vite": "^2.9.13",
    "vite-plugin-pages": "0.23.0",
    "vitest": "^0.18.1",
    "webpack": "5.44.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-emoji"
    }
  }
}
